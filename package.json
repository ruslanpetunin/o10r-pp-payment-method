{
  "name": "o10r-pp-payment-method",
  "version": "0.1.1",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruslanpetunin/o10r-pp-payment-method"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --clean --tsconfig tsconfig.build.json"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "o10r-pp-core": "^0.1.0",
    "o10r-pp-form": "^0.1.0"
  }
}
